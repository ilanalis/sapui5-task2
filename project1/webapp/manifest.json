{
  "_version": "1.76.0",
  "sap.app": {
    "id": "project1",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.19.3",
      "toolsId": "5ba983e2-7d82-4b1f-ac66-02805afe8b96"
    },
    "dataSources": {
      "ODataV2": {
        "uri": "/bookshop/v2/",
        "type": "OData",
        "settings": {
          "localUri": "localService/ODataV2/metadataV2.xml"
        }
      },
      "ODataV4": {
        "uri": "/bookshop/v4/",
        "type": "OData",
        "settings": {
          "localUri": "localService/ODataV4/metadataV4.xml",
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.142.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "project1.i18n.i18n"
        }
      },
      "ODataV2": {
        "dataSource": "ODataV2",
        "type": "sap.ui.model.odata.v2.ODataModel"
      },
      "ODataV4": {
        "dataSource": "ODataV4",
        "type": "sap.ui.model.odata.v4.ODataModel"
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "project1.view",
        "async": true,
        "viewPath": "project1.view"
      },
      "routes": [
        {
          "name": "RouteMain",
          "pattern": ":?query:",
          "target": ["TargetMain"]
        },
        {
          "name": "tab",
          "pattern": "tab/{tabName}",
          "target": ["TargetMain"]
        },
        {
          "pattern": "Product/{productPath}",
          "name": "product",
          "target": "Product"
        }
      ],
      "targets": {
        "TargetMain": {
          "id": "Main",
          "name": "Main"
        },
        "Product": {
          "id": "Product",
          "name": "Product"
        }
      }
    },
    "rootView": {
      "viewName": "project1.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}
